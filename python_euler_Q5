import math


def find_smallest_multiple(start_number):
  number_found = False
	current = start_number

	while number_found == False:
	
		for x in xrange(1,21):	
			if current%x != 0:
				#print "%d broke the loop"%(x, )	
				current += 1
				break					
			if x == 20:
				number_found = True
				print "The winning number is %d"%(current, )
			

find_smallest_multiple(232792500)
